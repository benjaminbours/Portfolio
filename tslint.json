{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended",
		"tslint-react-hooks"
	],
	"jsRules": {
		"object-literal-sort-keys": false
	},
	"rules": {
		"max-line-length": false,
		"no-console": false,
		"object-literal-sort-keys": false,
		"ordered-imports": false,
		"forin": false,
		"react-hooks-nesting": "error"
		// "interface-name": false
	},
	"rulesDirectory": [],
	"experimentalDecorators": true
}